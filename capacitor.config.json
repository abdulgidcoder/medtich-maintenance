{
  "appId": "com.arabcoderz.maintenanceeg",
  "appName": "Medtich Maintenance EG",
  "webDir": "./dist",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,
      "backgroundColor": "#ffffff",
      "androidSplashResourceName": "splash",
      "iosSplashResourceName": "splash",
      "androidScaleType": "CENTER",
      "showSpinner": true,
      "spinnerColor": "#00d9c8",
      "spinnerLines": 12,
      "spinnerCircles": 0,
      "spinnerDots": 0,
      "spinnerDuration": 1500
    }
  },
  "android": {
    "buildOptions": {
      "keystorePath": "./myapp.keystore",
      "keystorePassword": "WE#$%434e4v",
      "keystoreAlias": "key0",
      "keystoreAliasPassword": "WE#$%434e4v",
      "package": "com.arabcoderz.maintenanceeg",
      "releaseType": "AAB"
    }
  },
  "ios": {
    "limitsNavigationsToAppBoundDomains": true,
    "webContentsDebuggingEnabled": true
  },
  "server": {
    "hostname": "localhost",
    "cleartext": true,
    "allowNavigation": [
      "maintenance.medtich-eg.com"
    ]
  }
